size(500,500);
background(0,0,0);

// Main circle
fill(255,0,0);
arc(250,250,300,300,PI,4*PI);
// Left eye
fill(255,255,255);
arc(200,200,50,50,PI,4*PI);

fill(0,0,0);
arc(200,200,20,20,PI,4*PI);
// Right eye
fill(255,255,255);
arc(300,200,50,50,PI,4*PI);

fill(0,0,0);
arc(300,200,20,20,PI,4*PI);
// Nose
fill(255,255,255);
arc(250,260,50,75,PI,2*PI);

fill(0,0,0);
arc(240,260,12,25,PI,2*PI);

fill(0,0,0);
arc(260,260,12,25,PI,2*PI);
// Mouth
fill(255,255,255);
arc(250,320,100,50,PI*2,PI*3);
